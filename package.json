{
  "name": "nextjs-structured-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "$npm_execpath lint:eslint && $npm_execpath lint:prettier",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier -c './src/**/*.{js,jsx,ts,tsx}'",
    "fix": "$npm_execpath fix:eslint && $npm_execpath fix:prettier",
    "fix:eslint": "$npm_execpath lint:eslint -- --fix",
    "fix:prettier": "$npm_execpath lint:prettier -- -w",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.1.6-canary.14",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.30",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "autoprefixer": "^10.4.5",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6-canary.14",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
